<!DOCTYPE html>
<head>
</head>
<body>
    <script>
    
    function gugu(){ //함수 안에 반복문이 2개
        let n;
        while(true){
            n=parseInt(prompt('정수입력'));
            //1보다 작거나 9보다 크거나 수가 아닐 경우
            if(n<1 || n>9 || isNaN(n)){
                alert('1부터 9 사이의 정수입력');               
            }
            else{
                break;
            }            
        }        
        for(let i=1; i<=9; i++){
            document.write(`${n}*${i}=${n*i}<br>`)
        }
    }
    gugu()

    </script>
</body>
</html>

//함수를 설정하고 함수안에 정수입력과 무한반복문 while(true)와 for문을 이용하여 구구단 출력하기

<!DOCTYPE html>
<head>
</head>
<body>
    <script>
        function show(a,b){
            let str="";
            for(let i=1;i<=b;i++){
                str+=a;                
            }
            return str;
        }
    
    document.write(show('$',10));
        //출력문을 보고 함수 호출,함수의 이름,매개변수를 확인할수 있다.

    </script>
</body>
</html>

//함수를 이용하여 문자열 연결

<!DOCTYPE html>
<head>
</head>
<body>
    <script>
        // let 변수명  = function(){

        // }
        // 변수명();

        //function(){}
            //좀더 간결하게 작성하는 함수(화살표 함수)
            //1. 변수식 (변수명=함수명)
            let sum= function(a,b){
                return a+b;
            }

            alert(sum(3,4));


            //2. 화살표 함수!! react에서 많이 사용
            //function 키워드 없음(간결)

            let sum1=(a,b) =>{
                return a+b;
                    
            }
            alert(sum(3,4))
            //화살표 함수 사용시 function 키워드와 return 키워드 불필요
            // 화살표 앞이 매개변수, 화살표 뒤가 return, {}불필요
             let sum2=(a,b) => a+b;
                alert(sum2(3,4));


            // let 변수명 = (a,b) => {
            //     return a+b;
            // }

            //alret로 hi 출력하는 함수

             let pro=() =>{
             alert("hi");
             }
             pro();

            let profile=(a,b)=>{
                let msg=`${a},${b}` //,를 사용해 변수를 넣을수 없기에 `로 묶음
                return msg;
            }

            alert(profile('aa',3))




            

    </script>
</body>
</html>

//function 함수설정을 => 화살표 함수를 통해 간결화 할 수 있다.
// => 화살표 함수는 react에서 요긴하게 쓰인다. (중요!)
<!DOCTYPE html>
<head>
</head>
<body>
    <script>
        //데이터 유형
        //숫자,문자,undifined,객체(배열array), 논리(boolean)
        
        //배열 : 여러개의 값을 저장하는 공간
        //      인덱스 번호로 규칙을 줌 -> 0부터 시작

        let ary = [1,2,3,4,5];
        //[]가 배열의 특징
        let ary1 = [1,'a']
        //Js 에선 문자열,숫자열 섞을 수 있음
        document.write(typeof(ary)+ '<br>') //object로 출력됨
        document.write(ary[0] + ' ' + ary[1] + ' ' +ary[2] + '<br>');
        //출력문 1,2,3 ? 순서를 생각했을 때에 0부터 시작이기에 ary[0]출력은 1
        
        //배열 개수
        document.write(ary.length);
        //출력문 5 ? 1,2,3,4,5 총 다섯개

        //인덱스 번호는 0부터 시작하기 때문에 배열 갯수보다 작다.
        for(let i=0; i<ary.length; i++){
            console.log(ary[i]);
            //출력문 콘솔창에 인덱스 번호 0~4 -> 1,2,3,4,5
        }
        for(let i in ary){ //배열이 가지고있는 인덱스 값을 변수 i에 저장!
            console.log(i);
        }
        // 인덱스를 출력한다. in
        // 저장된 변수값을 출력하고싶을때엔 ary[i]를 삽입
    </script>
</body>
</html>

//array? [] 를 이용해 array설정 사용할 수 있다.
//for문으로 in 사용하여 배열이 가지고있는 인덱스 값을 변수에 저장할 수 있다.

<!DOCTYPE html>
<head>
</head>
<body>
    <script>
       let student={ // student라는 객체 안에 name, age, height, show 라는 변수 지정
      name : 'juli',
      age:42,
      height:188,
      weight:66,
      show : function(){
        console.log(`${student.name}`);
      }
    }
    student.show(); //함수호출      
    </script>
</body>
</html>

