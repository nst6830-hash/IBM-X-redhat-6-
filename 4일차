<!DOCTYPE html>
<head>
</head>
<body>
    <script>
       //기본 객체 선언방식
       //객체를 기준으로 해서 js 코드 구현 가능
       
        let car = { //객체명은 car
            name :'소나타', 
            color :['red','black','white'],
            fuel : '전기',

            operating : function(e){
                if(e=='on'){
                    alert('시동 on');
                }
                else if(e=='off'){
                    alert('시동 off');
                }
            }
        }
        console.log(car.color);//객체 car에 있는 name 이라는 변수 출력
        //배열시 객체명[인덱스]
        console.log(car.color[1]);
        console.log(car.fuel);


        //method (함수) 호출 -> 객체명.함수명
        console.log(car['name']);
        car.operating('on');
        
        


        //속성 호출 -> 객체명.속성명, 객체명[속석명]

        // function(){}
        // let n=
        // let m=()=>{}

        </script>
</body>
</html>

<!DOCTYPE html>
<head>
</head>
<body>
    <script>
    let student={
        name : 'mike',
        show : function(){
            console.log(student.name);
            
        }
    }
    student.show();
//객체명은 student, 객체 안 함수는 show()
    let student1={
    name : 'mike',
    show : function(){
            console.log(this.name);
            
        }
    }
    student.show();
    //this ? show 함수가 들어있는 객체를 의미
    
    </script>
</body>
</html>

//객체 안에 들어가 있는 함수를 구별하고 this를 이용해 객체안에있는 show 함수를 대신해 사용할 수 있음.
//this 객체 자신
//아래는 예시이다
<!DOCTYPE html>
<head>
</head>
<body>
    <script>
    //this : 객체 자신을 의미
    //1 기본 객체 구조
    let student ={
        name :'tom',
        test : function(){
            console.log(`${this.name}`);
            
        }
    }
    let student2 ={
        name :'tom2',
        test : function(){
            console.log(`${this.name}`);
            
        }
    }
    student.test(); //tom
    student2.test(); //tom2
    
    </script>
    </script>
    
</body>
</html>

//객체명이 변경 되어도 메소드 내에 객체명을 사용하지 않고 좀 더 효율적인 코드 진행-> this



<!DOCTYPE html>
<head>
</head>
<body>
    <script>
    //this : 객체 자신을 의미
    //1 기본 객체 구조
    let student ={
        name :'tom',
        test : test
                  
    }
    let student2 ={
        name :'tom2',
        test : test
}                           
         function test(){
            console.log(`${this.name}`);   
       }
    
    student.test();
    student2.test();

    </script>
    </script>
    
</body>
</html>

//위에 있는 코드 진행을 this를 통해 좀 더 간결하게 작성하기


<!DOCTYPE html>
<head>
</head>
<body>
    <script>
        //1. 기본 객체 구조
        
        
        //비슷한 객체를 여러개 만든다 ? 붕어빵 틀을 만들어 뒀다고 생각하면 된다.
        
        //2. 생성자 함수(비슷한 객체를 여러개 만들 때)this

        let adr='서울';//변경 사항이 없는 데이터는 함수에 넣지 않고 전역변수로 선언(const)

        function Item(product, price, deliver){
            this.product=product; //새로 생성되어지는 객체 자신
            this.price=price;
            this.deliver=deliver;
        
            this.show=function(){
                console.log(`${adr} ${product} 가격은 ${price}이고 배송은 ${deliver}`);
            } 
        }
        //실제 객체 생성코드
        //            Item(product,    price,   deliver) 먼저 설정된 Item이라는 함수로 아래에 연결
        let item1=new Item('핸드폰케이스',30000,'로켓배송')
        let item2=new Item('양말',20000,'새벽배송')

        item1.show();
        item2.show();
        





    </script>  
</body>
</html>

//생성자 함수(비슷한 객체)를 여러개 만들때에 this 이용하며, 
// 실제 객체 생성코드를 연결할 수 있다.

//아래는 위 코드를 변형해 응용한 이름과 시험점수 나열이다
<!DOCTYPE html>
<head>
</head>
<body>
    <script>


        function test (name, kor, eng, math){
            this.name=name; 
            this.kor=kor;
            this.eng=eng;
            this.math=math;

        
            this.average =function(){
                console.log(`${name}의 국어점수는 ${kor}영어점수는${eng}수학점수는${math}`);
            } 
        }
        //실제 객체 생성코드
        //            Item(product,    price,   deliver) 먼저 설정된 Item이라는 함수로 아래에 연결
        let a=new test('김길동',95,85,85)
        let b=new test('이길동',88,77,90)
        let c=new test('박길동',66,76,79)
        

        a.average();
        b.average();
        c.average();
        

    </script>  
</body>
</html>

<!DOCTYPE html>
<head>

</head>
<body>
    <script>
     function cafe (coffe){
        alert(coffe + '나왔습니다');
        //this.value ? 버튼 내 설정한 value를 함수호출 하기 위해 설정하는것
     }

    </script>
<!--버튼명은 한글, 버튼을 클릭시 cafe라는 함수가 호출 여기서 value값은 영문(실제 데이터)-->
    <button onclick="cafe(this.value)" value="americano">아메리카노</button>
    <button onclick="cafe(this.value)" value="latte">라떼</button>
    <button onclick="cafe(this.value)" value="smoothie">스무디</button>



    
</body>
</html>

//html 버튼을 이용하기


<!DOCTYPE html>
<head>

</head>
<body>
    
    
    <button onclick="food(this.value)" value="food1">라면</button>
    <button onclick="food(this.value)" value="food2">떡볶이</button>
    <button onclick="food(this.value)" value="food3">김밥</button>   
    <script>

               //함수명 

     let rest='신전떡볶이';
        //객체=변수+함수
    function Menu(title, price, num){
        this.title=title;
        this.price=price;
        this.num=num;

        this.result=function(){
            alert('분식집'+ rest);
            alert('선택한 메뉴 정보'+ this.title +','+ this.price+','+this.num );

        }
    }
    let food1= new Menu('떡라면',4500,1)
    let food2= new Menu('치즈떡볶이',12000,2)
    let food3= new Menu('참치김밥',10000,4)

        function food(v){
            if(v=='food1'){
                food1.result();

            }
            else if(v=='food2'){
                food2.result();

            }
            else if(v=='food3'){
                food3.result();
            }
        }
        
        //변수(속성) : 대상에 대한 정보 (크기,색상,모양 등)
        //함수(기능) : 대상이 가지고있는 기능 (음량조절, 채널변경, 전원끄고 켜기 등)

        //생성자함수 : 기본적인 틀을 구현하고 비슷한 객체를 생성하기 위함 



    </script>

</body>
</html>

//버튼을 만들어 생성자함수를 활용하기

//1. 1~4 사이의 정수를 입력 받아 prompt() 함수로 받아서 변수에 담는다
//switch 문을 사용해서 변수를 비교해 남녀를 판단해서 alert()로 출력한다.
//1~4 사이의 정수가 아니거나 문자이거나 하는 경우는 1~4사이의 정수를 입력하라고 alert()로 출력한다.

<!DOCTYPE html>
<head>

</head>
<body>
    <script>

    let a=parseInt(prompt('1~4 사이의 정수를 입력하세요'))
        if(isNaN(a)){
            alert('정수가 아닙니다.')
            }
            if(a<1 || a>4 ){
                alert('1~4 사이의 정수 입력')
            }
        
        
        switch(a){
            
             

            
            case 1 :
                alert('남자')
                break;
            case 2 :
                alert('여자')
                break;
            case 3 :
                alert('남자')
                break;
            case 4 :
                alert('여자')
                break;
            
                
        }
        
    </script>

</body>
</html>
