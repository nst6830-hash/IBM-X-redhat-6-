        document.write(isNaN(100));
        //출력하면 false 이유? 문자가 아니라 숫자열이기 때문
        document.write(isNaN("100"));
        //Num으로 자동 형 변환 -> 숫자열 
        document.write(isNaN('anc'));
        // 문자열이기 때문에 True
        document.write(isNaN(3.3));
