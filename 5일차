        console.log(result);
        console.log(typeof(result));
        console.log((result/3).toFixed(2)); //toFixed 소수점 자리 수 출력 설정 2




        let a = 44.64;
        let b = 22.22;

        //자리 올림 (무조건 올림)
        console.log(Math.ceil(a));
        console.log(Math.ceil(b));
        //자리 내림 (무조건 내림)
        console.log(Math.floor(a));
        console.log(Math.floor(b));
        
        //자리 반올림
        console.log(Math.round(a));
        console.log(Math.round(b));

        //******random******
        //0~1사의 수를 무작위로 반환(실수형태)
        let num=Math.random(); //100 < Math.random()*20 + 100 < 120
        console.log(num);

        let num1=Math.round(Math.random() * 9)+1; //1 < Math.random () < 10
        console.log(num1); 
        
        //50부터 80 까지 

        let num2=Math.floor(Math.random() *31)+50 ; 
        console.log(num2);





        //오늘 날짜,시간
        let day=new Date();
        console.log(day); //Mon Feb 09 2026 12:51:20 GMT+0900 (한국 표준시)

        //특정 날짜 시간
        let day2=new Date(2025,11,25);
        console.log(day2);
        //월이 하나 증가되서 출력됨.
        //0 ->1월
        //1 ->2월 
        // ...
        // 11->12 
        //월은 index로 정해져있음 Js에선 month 0부터 시작

        //오늘 날짜 출력

        console.log(day.toLocaleDateString());//2026.2.9.
        
        //오늘 시간만 출력
        console.log(day.toLocaleTimeString());//오후 1:03:37
        
        //오늘 날짜 시간 둘다 출력
        console.log(day.toLocaleString());//2026. 2. 9. 오후 1:03:37
