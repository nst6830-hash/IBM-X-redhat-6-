            //0 1 234 5 6 78910111213
        let n=new String("공부는 어렵지만, 재밌네요");
        //index 6에 있는 한 글자 출력

        document.write(n.charAt(6)+ "<br>"); //지
        //공부는 문자 출력 substr, slice

        document.write(n.substr(0,3)+ "<br>"); //0~2까지 출력
        document.write(n.slice(0,3)+ "<br>"); // 0~2까지 출력
        //split(separator)
        document.write(n.split(",")[0] + "<br>") //배열로 리턴된다.
        document.write(n.split(",")[1] + "<br>") //,를 기준으로 나누어 진다.
        //공부는 어렵지만 "," 재밌네요

        //split(separator,limit)
        document.write(n.split(",",1)+ "<br>") //,를 기준으로 나누되 1개 까지만 출력
        document.write(n.split(",",2)+ "<br>")
        document.write(n.split(",",3)+ "<br>")

