        // 

        const str1 = "Hello"
        const str2 = "World"

        console.log(str1.concat(str2)); //str1이 주체가 되어 str2 나열(연결)
        console.log(str2.concat(",",str1)); //str2를 기준으로 , 를 사용해서 srt1 나열

